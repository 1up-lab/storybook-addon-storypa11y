{
  "name": "storybook-addon-storypa11y",
  "version": "0.0.2",
  "description": "Storybook-addon-storypa11y is an addon for storybook that runs pa11y tests over each story in a storybook. It is based on top of the official storyshots addon",
  "keywords": [
    "addon",
    "storybook",
    "pa11y"
  ],
  "homepage": "https://github.com/tobilen/storybook-addon-storypa11y",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/tobilen/storybook-addon-storypa11y/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tobilen/storybook-addon-storypa11y.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "example": "jest index.test",
    "test": "build-storybook && jest",
    "build": "rollup -c",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@storybook/addons": "4.0.9",
    "global": "^4.3.2",
    "pa11y": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@storybook/addon-actions": "4.0.9",
    "@storybook/addon-links": "4.0.9",
    "@storybook/react": "4.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "raf": "^3.4.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.3",
    "rollup": "^0.67.3",
    "rollup-plugin-includepaths": "^0.2.3"
  }
